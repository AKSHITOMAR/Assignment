vector<int> Smallestonleft(int arr[], int n) {
    // code here
     set<int> s;
    vector<int> ans(n);
    for (int i = 0; i < n; i++) {
        auto it = s.lower_bound(arr[i]);
        if (it == s.begin()) ans[i] = -1;
        else {
            --it;
            ans[i] = *it;
        }
        s.insert(arr[i]);
    }
    return ans;

}
