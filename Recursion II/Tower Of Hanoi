class Solution {
  public:
    int towerOfHanoi(int n, int from, int to, int aux) {
        if(n==0){
            return 0;
        }
        long long moves=0;
        moves+=towerOfHanoi(n-1,from,aux,to);
        moves++;
        moves+=towerOfHanoi(n-1,to,from,aux);
        return moves;
    }
};
