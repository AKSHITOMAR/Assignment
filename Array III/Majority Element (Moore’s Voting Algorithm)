class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int>mp;
        for(int x:nums){
            mp[x]++;
        }
        int maxi=INT_MIN;
        for(auto it: mp){
            if(maxi<it.second){
                maxi=it.second;
            }
        }
        for(auto it:mp){
            if(maxi==it.second){
                return it.first;
            }
        }
        return -1;
    }
};
