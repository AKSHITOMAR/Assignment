#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    getline(cin,str);
    int n=str.length();
    int vowels=0,consonants=0,space=0;
    for(int i=0;i<n;i++){
        if(str[i]==' '){
            space++;
        }
        else if (str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||
                str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U'){
            vowels++;
        }
        else{
            consonants++;
        }
    }
    cout<<"Vowels is "<<vowels<<endl;
    cout<<"Consonants is: "<<consonants<<endl;
    cout<<"Space is "<<space<<endl;

}


