# APPROACH 1
#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int key;
    cin>>key;
    for(int i=0;i<n;i++){
        if(arr[i]==key){
            cout<<i+1;
        }
    }

# APPROACH 2

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int key;
    cin>>key;
    int start=0;
    int end=n-1;
    sort(arr.begin(),arr.end());
    while(start<=end){
        int mid=start+(end-start)/2;
        if(arr[mid]==key){
            cout<<"Element found at index "<<mid;
            return 0;
        }
        else if(arr[mid]<key){
            start=mid+1;
        }
        else{
            end=mid-1;
        }
    }
    cout<<"Element not found";
    return 0;
}
