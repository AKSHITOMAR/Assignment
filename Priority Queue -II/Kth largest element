class Solution {
public:
    int findKthLargest(vector<int>& nums, int k) {
      priority_queue<int>que;
      for(int i=0;i<nums.size();i++){
        que.push(nums[i]);
      } 
      vector<int>num;
      for(int i=1;i<k;i++){
        num.push_back(que.top());
        que.pop();
      }
      return que.top();
    }
};
