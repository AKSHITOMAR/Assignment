class Solution {
public:
    bool isAnagram(string s, string t) {
        unordered_map<char,int>mps;
        unordered_map<char,int>mpt;
        if(s.size()!=t.size()){
            return false;
        }
        for(char c:s){
            mps[c]++;
        }
        for(char c:t){
            mpt[c]++;
        }
        for(auto stt:mps){
            char c=stt.first;
            int val1=stt.second;
            if(mpt.find(c)==mpt.end()){
                return false;
            }
            else
            {
            int val2=mpt[c];
            if(val1!=val2)
            return false;
            }
        }
        return true;
    }
};
