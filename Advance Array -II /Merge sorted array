#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int m;
    cin>>m;
    vector<int>brr(m);
    for(int i=0;i<m;i++){
        cin>>brr[i];
    }
    vector<int>crr;
    for(int i=0;i<n;i++){
        crr.push_back(arr[i]);
    }
    for(int i=0;i<m;i++){
        crr.push_back(brr[i]);
    }
    sort(crr.begin(),crr.end());
    for(int i=0;i<crr.size();i++){
        cout<<crr[i]<<" ";
    }
}
