class Solution {
public:
    int maxArea(vector<int>& height) {
        int i=0,j=height.size()-1,totalarea=0;
        while(i<j){
            int breadth=j-i;
            int heighthh=min(height[i],height[j]);
            int area=breadth*heighthh;
            totalarea=max(totalarea,area);
            if(height[i]<height[j]){
                i++;
            }
            else{
                j--;
            }
        }
        return totalarea;
    }
};
