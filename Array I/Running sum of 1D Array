# APPROACH 1 

#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    vector<int>ans;
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=arr[i];
        ans.push_back(sum);
    }
    for(int i=0;i<n;i++){
        cout<<ans[i]<<" ";
    }
}

# APPROACH 2 :-
class Solution {
public:
    vector<int> runningSum(vector<int>& nums) {
        vector<int>result(nums.size());
        result[0]=nums[0];
        for(int i=1;i<nums.size();i++){
            result[i]=result[i-1]+nums[i];
        }
        return result;
    }
};
