# APPROACH 1
#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin>>n;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int maxi=INT_MIN;
    int mini=INT_MAX;
    for(int i=0;i<n;i++){
        if(arr[i]>maxi){
            maxi=arr[i];
        }
        if(arr[i]<mini){
            mini=arr[i];
        }
    }
    cout<<"Maximum Element is:"<<maxi<<endl;
    cout<<"Minimum Element is:"<<mini<<endl;
}

APPROACH 2 ON GFG 
class Solution {
  public:
    vector<int> getMinMax(vector<int> &arr) {
        int n=arr.size();
        int mini=INT_MAX;
        int maxi=INT_MIN;
        for(int i=0;i<n;i++){
            if(arr[i]>maxi){
                maxi=arr[i];
            }
            if(arr[i]<mini){
                mini=arr[i];
            }
        }
        return {mini,maxi};
    }
};
