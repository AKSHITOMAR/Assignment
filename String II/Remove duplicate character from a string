#include<bits/stdc++.h>
using namespace std;
int main(){
    string str;
    cin>>str;
    int n=str.length();
    set<int>s;
    string result="";
    for(int i=0;i<n;i++){
        char c=str[i];
        if(s.find(c)==s.end()){
            s.insert(c);
            result+=c;
        }
    }
    cout<<result;
}


